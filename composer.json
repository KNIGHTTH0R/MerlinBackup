{
    "name": "ucsdmath/merlin-backup",
    "type": "library",
    "description": "A database backup handler used for storing backups locally and remotely to tables and files.",
    "keywords": ["merlin-backup", "ucsdmath", "backup", "mysql", "component"],
    "homepage": "https://github.com/ucsdmath",
    "license": "MIT",
    "time": "2017-02-26",
    "authors": [
        {
            "name": "Daryl Eisner",
            "email": "deisner@ucsd.edu"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ucsdmath/filesystem": "1.16.x-dev",
        "ucsdmath/configuration-vault": "1.16.x-dev"
    },
    "require-dev": {
        "symfony/debug": "3.2.5",
        "mockery/mockery": "0.9.9",
        "phpunit/phpunit": "6.0.8",
        "fzaninotto/faker": "1.6.0",
        "ucsdmath/testing": "1.16.x-dev",
        "symfony/var-dumper": "3.2.5",
        "symfony/browser-kit": "3.2.5",
        "symfony/dom-crawler": "3.2.5",
        "symfony/options-resolver": "3.2.5",
        "squizlabs/php_codesniffer": "2.8.1"
    },
    "autoload": {
        "psr-4": {
            "UCSDMath\\MerlinBackup\\": "src/MerlinBackup/"
        }
    },
    "suggest": {
        "symfony/dependency-injection": "Symfony DependencyInjection Component for fast loading of classes"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.16-dev"
        }
    },
    "minimum-stability": "dev"
}
