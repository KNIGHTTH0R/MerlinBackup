{
    "name": "ucsdmath/merlin-backup",
    "type": "library",
    "description": "A database backup handler used for storing backups locally and remotely to tables and files.",
    "keywords": ["merlin-backup", "ucsdmath", "backup", "mysql", "component"],
    "homepage": "https://github.com/ucsdmath",
    "license": "MIT",
    "time": "2017-10-15",
    "authors": [
        {
            "name": "Daryl Eisner",
            "email": "deisner@ucsd.edu"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ucsdmath/sftp": "1.28.x-dev",
        "ucsdmath/filesystem": "1.28.x-dev",
        "ucsdmath/configuration-vault": "1.28.x-dev"
    },
    "require-dev": {
        "symfony/debug": "3.3.10",
        "mockery/mockery": "1.0.0",
        "phpunit/phpunit": "6.4.3",
        "fzaninotto/faker": "1.7.1",
        "ucsdmath/date": "1.28.x-dev",
        "symfony/var-dumper": "3.3.10",
        "symfony/browser-kit": "3.3.10",
        "symfony/dom-crawler": "3.3.10",
        "symfony/options-resolver": "3.3.10",
        "squizlabs/php_codesniffer": "3.1.0"
    },
    "autoload": {
        "psr-4": {
            "UCSDMath\\MerlinBackup\\": "src/MerlinBackup/"
        }
    },
    "suggest": {
        "symfony/dependency-injection": "Symfony DependencyInjection Component for fast loading of classes"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.28-dev"
        }
    },
    "minimum-stability": "dev"
}
