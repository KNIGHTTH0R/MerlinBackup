{
    "name": "ucsdmath/merlin-backup",
    "type": "library",
    "description": "A database backup handler used for storing backups locally and remotely to tables and files.",
    "keywords": ["merlin-backup", "ucsdmath", "backup", "mysql", "component"],
    "homepage": "https://github.com/ucsdmath",
    "license": "MIT",
    "time": "2017-12-14",
    "authors": [
        {
            "name": "Daryl Eisner",
            "email": "deisner@ucsd.edu"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ucsdmath/sftp": "2.2.x-dev",
        "ucsdmath/filesystem": "2.2.x-dev",
        "ucsdmath/configuration-vault": "2.2.x-dev"
    },
    "require-dev": {
        "symfony/debug": "4.0.2",
        "mockery/mockery": "1.0.0",
        "phpunit/phpunit": "6.5.5",
        "fzaninotto/faker": "1.7.1",
        "ucsdmath/date": "2.2.x-dev",
        "symfony/var-dumper": "4.0.2",
        "symfony/browser-kit": "4.0.2",
        "symfony/dom-crawler": "4.0.2",
        "symfony/options-resolver": "4.0.2",
        "squizlabs/php_codesniffer": "3.2.2"
    },
    "autoload": {
        "psr-4": {
            "UCSDMath\\MerlinBackup\\": "src/MerlinBackup/"
        }
    },
    "suggest": {
        "symfony/dependency-injection": "Symfony DependencyInjection Component for fast loading of classes"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    },
    "minimum-stability": "dev"
}
